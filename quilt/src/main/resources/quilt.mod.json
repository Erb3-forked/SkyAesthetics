{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "sky_aesthetics",
    "version": "${version}",
    "metadata": {
      "name": "Sky Aesthetics",
      "description": "Change the skybox of Minecraft like you want !",
      "contributors": {
        "TATHAN": "Author"
      },
      "icon": "assets/sky_aesthetics/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "fr.tathan.quilt.ExampleModQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.21"
      }
    ]
  },
  "mixin": [
    "sky_aesthetics.mixins.json"
  ]
}
